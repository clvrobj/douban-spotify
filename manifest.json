{
  "name": "豆瓣Spotify助手",
  "version": "1.3",
  "description": "帮你从豆瓣专辑转到Spotify收听，是时候清理你的想听列表啦~",
  "browser_action": {
     "icons": ["spotify.png"]
  },
  "content_scripts": [{
      "matches": ["http://music.douban.com/people/*/do*",
                  "http://music.douban.com/people/*/wish*",
                  "http://music.douban.com/people/*/collect*"],
      "run_at": "document_end",
      "css":["doubanspotify.css"],
      "js": ["jquery-1.7.1.min.js", "page.js"],
      "all_frames": true
    },
    {
      "matches": ["http://music.douban.com/subject/*/"],
      "run_at": "document_end",
      "css":["doubanspotify.css"],
      "js": ["jquery-1.7.1.min.js", "subject.js"],
      "all_frames": true
    }],
  "icons": {
    "16": "spotify_16.png",
    "48": "spotify_48.png"
  }
}